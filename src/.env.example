# Server configuration
PORT=3000

# GitHub Enterprise configuration
GH_ENTERPRISE_URL=https://github.example.com
GH_ENTERPRISE_API_URL=https://github.example.com/api/v3

# Authentication method: 'oauth' or 'token'
GH_AUTH_METHOD=token

# For OAuth authentication
GH_CLIENT_ID=your_github_oauth_client_id
GH_CLIENT_SECRET=your_github_oauth_client_secret
GH_CALLBACK_URL=http://localhost:3000/auth/github/callback

# For token authentication (Personal Access Token)
GH_TOKEN=your_github_personal_access_token
GH_USERNAME=your_github_username

# Session configuration
SESSION_SECRET=your_session_secret
